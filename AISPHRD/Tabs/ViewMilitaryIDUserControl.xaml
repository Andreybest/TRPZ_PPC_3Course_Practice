<local:TabUserControl x:Class="AISPHRD.Tabs.ViewMilitaryIDUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AISPHRD.Tabs"
             mc:Ignorable="d" 
             d:DesignHeight="500" d:DesignWidth="1080" Loaded="TabUserControl_Loaded">
    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
        <Image x:Name="MilitaryIDImage" HorizontalAlignment="Center" VerticalAlignment="Center"/>

        <Button x:Name="CloseButton" Content="Закрити" HorizontalAlignment="Center"  VerticalAlignment="Bottom" Margin="0,0,0,20" Click="CloseButton_Click"/>
    </Grid>
</local:TabUserControl>
